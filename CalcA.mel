global proc Multiplication()
{
    $answer = ($num1 *= $num2);
    print ($num1 + " * " + $num2 + " = " + $answer);
    return ($answer);

}

global proc Division()
{
    $answer = ($num1 /= $num2);
    print ($num1 + " + " + $num2 + " = " + $answer);
    return ($answer);

}

global proc Power()
{
    $answer = ($num1 ^= $num2);
    print ($num1 + " + " + $num2 + " = " + $answer);
    return ($answer);

}

